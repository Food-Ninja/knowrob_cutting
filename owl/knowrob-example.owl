    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ease-crc.org/ont/knowrob-example#Cheese -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/knowrob-example#Cheese">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/knowrob-example#ProcessedFood"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/knowrob-example#Ingredient -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/knowrob-example#Ingredient">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalBody"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/knowrob-example#Pizza -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/knowrob-example#Pizza">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Dish"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/knowrob-example#hasIngredient"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/knowrob-example#Cheese"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/knowrob-example#hasIngredient"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/knowrob-example#Tomato"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A large circle of flat bread baked with cheese, tomatoes, and sometimes meat and vegetables spread on top</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/knowrob-example#ProcessedFood -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/knowrob-example#ProcessedFood">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/knowrob-example#Ingredient"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/knowrob-example#Tomato -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/knowrob-example#Tomato">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/knowrob-example#Vegetable"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/knowrob-example#Vegetable -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/knowrob-example#Vegetable">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/knowrob-example#Ingredient"/>
    </owl:Class>
