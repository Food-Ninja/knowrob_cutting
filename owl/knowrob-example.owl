    <!-- http://www.ease-crc.org/ont/knowrob-example#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/knowrob-example#hasIngredient">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#hasPhysicalComponent"/>
        <rdfs:comment>A component relation between a dish and food or substances needed to create the dish.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
